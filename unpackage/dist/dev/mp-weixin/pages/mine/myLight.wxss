
.container {
	width: 100%;
}
.gfLogo {
	position: relative;
	display: inline-block;
	width: 100%;
	height: 270rpx;
	background-color:#f8f8f8; 
	/* background: url("../../static/imgs/gf/logo.jpg"); */
}
.gfLogo image{
	display: block;
	width: 100%;
	height: 270rpx;
	border-top-right-radius: 30rpx;
	border-top-left-radius: 30rpx;
}
.nav {
	position: relative;
	width: 100%;
	height: 180rpx;
	margin-top: -30rpx;
	background-color: #F1F1F1;
	border-top-right-radius: 30rpx;
	border-top-left-radius: 30rpx;
	z-index: 1;
}
.nav .li {
	float: left;
	width: 25%;
	text-align: center;
	font-size: 25rpx;
}
.li image{
	display: block;
	width: 75rpx;
	height: 75rpx;
	margin: 25rpx auto;
}
.ccon{
	width: 100%;
	margin-top: 25rpx;
	background-color: #F5F2F0;
}
.ccon view{
	float: left;
	width: 42.5%;
	height: 250rpx;
	margin-top: 35rpx;
	margin-left: 5%;
	text-align: center;
	background-color: #ffffff;
	border-radius: 20rpx;
}
.ccon::after{
	content:'';
	display: block;
	clear: both;
}
.ccon  image{
	display: block;
	margin: 35rpx auto;
	width: 25%;
	height: 75rpx;
}
.bottomm{
	margin-bottom: 35rpx;
}
.ccon text{
	display: block;
	margin-top:20rpx ;
}


